<?xml version="1.0" encoding="utf-8"?>
<Defs>


  <WorldObjectDef>
    <defName>ShipTracker</defName>
    <description>Ship Tracker. This shouldn't be visible</description>
    <worldObjectClass>OHUShips.ShipTracker</worldObjectClass>
    <canHaveFaction>false</canHaveFaction>
	<selectable>false</selectable>
	<neverMultiSelect>true</neverMultiSelect>
	<useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>false</expandingIcon>
    <texture>World/WorldObjects/Caravan</texture>
  </WorldObjectDef>
  
    <WorldObjectDef>
    <defName>WorldShip</defName>
    <label>worldship</label>
    <description>Various aircraft in flight.</description>
    <worldObjectClass>OHUShips.WorldShip</worldObjectClass>
    <texture>World/WorldObjects/Caravan</texture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/Expanding/Caravan</expandingIconTexture>
    <expandingIconPriority>100</expandingIconPriority>
    <expandMore>true</expandMore>
    <allowCaravanIncidentsWhichGenerateMap>true</allowCaravanIncidentsWhichGenerateMap>
    <IncidentTargetTags>
      <li>Caravan</li>
    </IncidentTargetTags>
    <inspectorTabs>
      <li>OHUShips.WITab_Worldship_Cargo</li>
    </inspectorTabs>
  </WorldObjectDef>

  <WorldObjectDef>
    <defName>ShipDropSite</defName>
    <label>aircraft dropsite</label>
    <description>A dropsite for aircraft.</description>
    <worldObjectClass>OHUShips.ShipDropSite</worldObjectClass>
    <texture>World/WorldObjects/AircraftDropSpot</texture>
    <expandingIcon>true</expandingIcon>
    <expandingIconTexture>World/WorldObjects/AircraftDropSpot</expandingIconTexture>
    <useDynamicDrawer>true</useDynamicDrawer>
    <expandingIconPriority>20</expandingIconPriority>
  </WorldObjectDef>


  
</Defs>